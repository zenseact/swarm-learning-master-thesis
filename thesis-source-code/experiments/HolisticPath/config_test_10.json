{
    "data": {
        "path": "/mnt/ZOD",
        "version": "full",
        "ratio": 0.006,
        "img_size": [
            400,
            1420
        ],
        "transforms": "[CustomCrop(800, 500, 800, 2840), Resize((400, 1420)), Normalize(mean=[0.485, 0.456, 0.406], std=[0.229, 0.224, 0.225])]",
        "shuffle_seed": 500,
        "decentralised_shuffle_seed": 42,
        "dataloader_args": {
            "batch_size": 4,
            "shuffle": true
        },
        "train_val_id_generator": "balanced_frames_borrowed",
        "dataset_getitem": "interpolated_target_distances_short"
    },
    "model": {
        "name": "resnet18-melted",
        "args": {
            "num_output": 51
        },
        "loss": "L1Loss",
        "optimiser": "Adam",
        "optimiser_args": {
            "lr": 0.01
        }
    },
    "central": {
        "train": true,
        "use_gpu": true,
        "epochs": 15
    }
}